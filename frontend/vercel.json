{
    "version": 2,
    "builds": [
        {
            "src": "frontend/src/index.js",
            "use": "@vercel/node",
            "config": {
                "distDir": "frontend/build"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "hrtalksdz-backend/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/build/$1"
        }
    ]
}
